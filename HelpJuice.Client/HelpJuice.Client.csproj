<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Clint Masden</Authors>
    <Company>NA</Company>
    <Product>HelpJuice Client [API Version 3]</Product>
    <PackageProjectUrl>https://github.com/clintmasden/helpjuice.client</PackageProjectUrl>
    <Description>A client for HelpJuice API version 3.</Description>
    <RepositoryUrl>https://github.com/clintmasden/helpjuice.client</RepositoryUrl>
    <PackageTags>HelpJuice Client API helpjuice.com</PackageTags>
    <Version>1.0.0.1</Version>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageReleaseNotes>implemeneted single sign on with json web token</PackageReleaseNotes>
    <PackageLicenseFile>mit-license.txt</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="6.16.0" />
    <PackageReference Include="System.Net.Http.Json" Version="6.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\mit-license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
